Scanner in = new Scanner (System.in);
        int numerorecebido = 0;
        System.out.println("DIGITE UM NÃšMERO QUE VOCE QUEIR AVER A TABUADA");
        numerorecebido = in.nextInt();
        int result = 0;
        
        for (int i=0;i<10;i++){
             result = i*numerorecebido;
             System.out.println(numerorecebido + " X " + i + " = " + result); 
        }
        
    }
    
}
